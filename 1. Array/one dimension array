#include <stdio.h>

void a(int no[]) {
    for (int i = 0; i < 5; i++) {
        printf("Enter number %d: ", i + 1);
        scanf("%d", &no[i]);
    }
}

void b(int no[]) {
    for (int i = 0; i < 5; i++) {
        printf("Number %d: %d\n", i + 1, no[i]);
    }
}

int main() {
    int no[5];
    a(no);
    b(no);
    return 0;
}
